{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "backend/flask_api/main/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/_nuxt/(.*)",
      "dest": "backend/flask_api/main/dist/_nuxt/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "backend/flask_api/main/dist/img/$1"
    },
    {
      "src": "/(favicon.ico|robots.txt)",
      "dest": "backend/flask_api/main/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}